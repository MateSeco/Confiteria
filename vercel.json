{
    "version": 2,
    "builds": [
         
    { "src": "index.js", "use": "@vercel/node-server" },
    { "src": "/public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
    {
    "src": "/css/(.*)",
    "dest": "/public/css/$1"
    },
    {
    "src": "/(.*)",
    "dest": "/index.js"
    }
    ]
   }
   